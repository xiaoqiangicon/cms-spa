{
  "name": "cms-spa",
  "private": true,
  "version": "0.0.1",
  "description": "A lila project.",
  "scripts": {
    "//precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"./**/*.{js,jsx,vue,css,less,scss,sass,html,md,json}\"",
    "eslint": "eslint \"./**/*.js\"",
    "eslint:fix": "eslint \"./**/*.js\" --fix",
    "eslint:react": "eslint \"./**/*.jsx\" -c .eslintrc.react.js",
    "eslint:react:fix": "eslint \"./**/*.jsx\" -c .eslintrc.react.js --fix",
    "eslint:vue": "eslint \"./**/*.vue\" -c .eslintrc.vue.js",
    "eslint:vue:fix": "eslint \"./**/*.vue\" -c .eslintrc.vue.js --fix",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix",
    "test": "jest test",
    "dev": "lila dev"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.3",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lila-bin": "^0.5.6",
    "lila-core": "^0.5.6",
    "lila-tasks": "^0.5.6",
    "lila-webpack": "^0.5.6",
    "lila-webpack-config": "^0.5.6",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^4.1.3"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.jsx": [
      "eslint -c .eslintrc.react.js --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.vue": [
      "eslint -c .eslintrc.vue.js --fix",
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,less,scss,sass}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@senntyou/shortcut.css": "^0.1.0",
    "element-ui": "^2.4.11",
    "js-cookie": "^2.2.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "particles.js": "^2.0.0",
    "vue": "^2.5.22",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  }
}
