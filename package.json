{
  "name": "cms-spa",
  "version": "0.1.0",
  "description": "A lila project.",
  "private": true,
  "scripts": {
    "precommit": "npm run lint-staged",
    "lint-staged": "lint-staged",
    "prettier": "prettier --write \"./**/*.{js,vue,css,less,scss,sass,html,md,json}\"",
    "eslint": "eslint \"./**/*.js\"",
    "eslint:fix": "eslint \"./**/*.js\" --fix",
    "eslint-vue": "eslint \"./**/*.vue\" -c .eslintrc.vue.js",
    "eslint-vue:fix": "eslint \"./**/*.vue\" -c .eslintrc.vue.js --fix",
    "stylelint": "stylelint \"./**/*.{css,less,scss,sass,vue}\"",
    "stylelint:fix": "stylelint \"./**/*.{css,less,scss,sass,vue}\" --fix",
    "flow": "flow",
    "flow:check": "flow check",
    "test": "jest",
    "test-watch": "jest --watch --notify",
    "test-cover": "jest --coverage",
    "coverage": "npm run test-cover && opn coverage/lcov-report/index.html",
    "dev": "lila dev",
    "sync": "lila sync",
    "sync:gray": "lila sync -e gray",
    "sync:prod": "lila sync -e prod"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.1",
    "eslint": "^5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.4.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^22.1.3",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-vue": "^5.2.2",
    "flow-bin": "^0.94.0",
    "husky": "^1.3.1",
    "jest": "^23.6.0",
    "lila-bin": "^0.7.1",
    "lila-core": "^0.7.1",
    "lila-tasks": "^0.7.1",
    "lila-webpack": "^0.7.1",
    "lila-webpack-config": "^0.7.2",
    "lint-staged": "^8.1.0",
    "moment-locales-webpack-plugin": "^1.0.7",
    "opn-cli": "^4.0.0",
    "prettier": "^1.15.3",
    "stylelint": "^9.9.0",
    "stylelint-config-prettier": "^4.0.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^4.1.3"
  },
  "lint-staged": {
    "**/*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.vue": [
      "eslint -c .eslintrc.vue.js --fix",
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,less,scss,sass}": [
      "stylelint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{html,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "dependencies": {
    "@fancyapps/fancybox": "^3.5.7",
    "@fortawesome/fontawesome-free": "^5.6.3",
    "@senntyou/shortcut.css": "^0.1.0",
    "@zzh/handling": "^0.3.0",
    "@zzh/regions": "^0.3.0",
    "@zzh/store-image": "git+http://git.zizaihome.cn/senntyou/store-image.git#ff9621e2",
    "@zzh/ueditor": "^0.3.0",
    "@zzh/upload": "git+http://git.zizaihome.cn/senntyou/upload.git#5fa55966",
    "@zzh/util": "^0.3.0",
    "colors.css": "^3.0.0",
    "element-ui": "^2.4.11",
    "jquery": "^3.3.1",
    "js-cookie": "^2.2.0",
    "json-refactor": "^1.0.0",
    "lunar-calendar": "^0.1.4",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "nprogress": "^0.2.0",
    "particles.js": "^2.0.0",
    "reqwest": "^2.0.5",
    "screenfull": "^4.0.0",
    "see-ajax": "^1.0.1",
    "see-fetch": "^1.0.1",
    "vue": "^2.5.22",
    "vue-i18n": "^8.7.0",
    "vue-router": "^3.0.2",
    "vuex": "^3.0.1"
  }
}
